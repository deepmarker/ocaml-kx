(library
 (name        kx_final)
 (public_name kx-final)
 (modules Kx_final)
 (synopsis "OCaml client for kdb+")
 (c_names k_stubs c)
 (c_flags -DKXVER=3 -gdwarf-2 -g3)
 (c_library_flags -lpthread)
 (libraries bigarray bigstring uuidm ptime uri))

(library
 (name        kx_async)
 (public_name kx-async)
 (modules Kx_async)
 (synopsis "OCaml client for kdb+, for Jane Street's async")
 (libraries kx-final core async logs-async))
